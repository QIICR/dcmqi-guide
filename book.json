 {
    "plugins": [
        "anchorjs",
        "-lunr",
        "-search",
        "search-plus",
        "versions",
        "etoc"        
    ],
    "pluginsConfig": {
        "anchorjs": {
            "placement": "right",
            "visible": "hover"
        },
        "versions": {
            "gitbookConfigURL": "https://raw.githubusercontent.com/QIICR/dcmqi-guide/master/book.json",
            "options": [
            {
                "value": "https://qiicr.gitbooks.io/dcmqi-guide/content/",
                "text": "latest",
                "selected": true
            },
            {
                "value": "https://qiicr.gitbooks.io/dcmqi-guide/content/v/v1.0.0/",
                "text": "v1.0.0"
            }]
        },
        "page-toc": {
            "position": "before-first"
        },
        "etoc": {
            "mindepth": 2,
            "maxdepth": 4,
            "notoc": false
        }
    }
}